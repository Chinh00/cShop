<Project Sdk="Microsoft.NET.Sdk.Web">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <Nullable>disable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
        <OutputType>Library</OutputType>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Google.Protobuf" Version="3.28.2" />
        <PackageReference Include="Grpc.Tools" Version="2.66.0">
          <PrivateAssets>all</PrivateAssets>
          <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
        </PackageReference>
        <PackageReference Include="Microsoft.AspNetCore.OpenApi" Version="8.0.8"/>
        <PackageReference Include="Swashbuckle.AspNetCore" Version="6.7.3"/>
    </ItemGroup>

    
    
    <ItemGroup>
      <None Remove="Properties\launchSettings.json" />
    </ItemGroup>
    <ItemGroup>

        <ProjectReference Include="..\..\..\..\BuildingBlock\cShop.Contracts\cShop.Contracts.csproj" />

        <ProjectReference Include="..\Projections\Projections.csproj" />

        <Protobuf Include="..\..\..\..\BuildingBlock\cShop.Contracts\Protos\Product\product.proto" Link="Protos\product.proto" GrpcServices="server" />
    </ItemGroup>

</Project>
